<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
  
<hibernate-mapping>  
    <class name="main.java.ch.wimf.product.Item" table="u400">
        <id name="id" type="int" column="id">
            <generator class="increment"></generator>  
        </id>
        <property name="id_fridge" type="java.lang.String"></property>
        <property name="name" type="java.lang.String"></property>  
        <property name="description" type="java.lang.String"></property>  
        <property name="barcode" type="java.lang.String"></property>    
        <property name="barcode_type" type="java.lang.String"></property>    
        <property name="category" type="java.lang.String"></property>    
        <property name="expiry_date" type="java.lang.String"></property>    
        <property name="creation_date" type="java.lang.String"></property>    
        <property name="quantity" type="java.lang.String"></property>    
        <property name="quantity_unit" type="java.lang.String"></property>  
    </class>  
         
</hibernate-mapping>  